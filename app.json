{
  "expo": {
    "name": "Rick and Morty",
    "slug": "rick-and-morty-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/rick.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "com.muskaracuha.rickandmortyapp"
    },
    "android": {
      "userInterfaceStyle": "light",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/rick.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.muskaracuha.rickandmortyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/rick.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/rick.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7c8decfd-3568-4241-9337-1c6905c58779"
      }
    },
    "owner": "muskaracuha"
  }
}
